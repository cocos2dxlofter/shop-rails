    <tr>
      <th>Product</th>
      <th>Sku code</th>
      <th>Price</th>
      <th>Quantity</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @product_skus.each do |product_sku| %>
      <tr>
        <td><%= product_sku.product %></td>
        <td><%= product_sku.sku_code %></td>
        <td><%= product_sku.price_yuan %>å…ƒ</td>
